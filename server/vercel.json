{
  "version": 2,
  "routes": [
      {
          "src": "/(.*)",
          "dest": "api/index.js",
          "methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
          "headers":{
              "Access-Control-Allow-Origin": "*"
          }
      }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 60
    }
  }
}